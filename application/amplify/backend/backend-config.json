{
  "auth": {
    "universalStoreAuth": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "universalStoreAuthPreSignup",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ],
      "customAuth": false
    }
  },
  "function": {
    "universalStoreAuthPreSignup": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "getShopifyAccessToken": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "shopifyAccessTokenDB",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "storage": {
    "shopifyAccessTokenDB": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "api": {
    "shopifyAccessTokenAPI": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "getShopifyAccessToken",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  }
}